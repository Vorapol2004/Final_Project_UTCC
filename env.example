# MySQL Configuration
# IMPORTANT: Change these values for your environment!
# Copy this file to .env and set your actual credentials
MYSQL_ROOT_PASSWORD=your_root_password_here
MYSQL_USER=your_mysql_username
MYSQL_PASSWORD=your_mysql_password
MYSQL_PORT=3307

# Spring Boot Configuration
SPRING_BOOT_PORT=8080
# Generate a strong random JWT secret (at least 32 characters)
# You can use: openssl rand -base64 32
JWT_SECRET=your_jwt_secret_key_here_minimum_32_characters

# Next.js Configuration
NEXTJS_PORT=3000
NEXT_PUBLIC_API_URL=http://localhost:8080

# Database Configuration for Spring Boot (local development)
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password

# CORS Configuration
CORS_ORIGINS=http://localhost:3000

